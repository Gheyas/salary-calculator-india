{
  "routes": [
    {
      "route": "/about",
      "rewrite": "/about.html"
    },
    {
      "route": "/contact",
      "rewrite": "/contact.html"
    },
    {
      "route": "/faq",
      "rewrite": "/faq.html"
    },
    {
      "route": "/privacy",
      "rewrite": "/privacy.html"
    },
    {
      "route": "/pay-commission",
      "rewrite": "/pay-commission.html"
    },
    {
      "route": "/blog",
      "rewrite": "/blog.html"
    },
    {
      "route": "/*.{js,css,png,jpg,jpeg,gif,svg,woff,woff2}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}